{
    "id": "gitlab",
    "container": {
      "type": "DOCKER",
      "volumes": [
        {
          "persistent": {
            "type": "root",
            "size": 1000,
            "constraints": []
          },
          "mode": "RW",
          "containerPath": "git-data"
        },
        {
          "containerPath": "/var/opt/gitlab/git-data",
          "hostPath": "/mnt/srv/gitlab-data/git-data/",
          "mode": "RW"
        },
        {
          "persistent": {
            "type": "root",
            "size": 1000,
            "constraints": []
          },
          "mode": "RW",
          "containerPath": "home"
        },
        {
          "containerPath": "/var/opt/gitlab",
          "hostPath": "/mnt/srv/gitlab-data/home/",
          "mode": "RW"
        },
        {
          "persistent": {
            "type": "root",
            "size": 1000,
            "constraints": []
          },
          "mode": "RW",
          "containerPath": "upload"
        },
        {
          "containerPath": "/var/opt/gitlab/gitlab-rails/uploads",
          "hostPath": "/mnt/srv/gitlab-data/uploads/",
          "mode": "RW"
        },
        {
          "persistent": {
            "type": "root",
            "size": 1000,
            "constraints": []
          },
          "mode": "RW",
          "containerPath": "shared"
        },
        {
          "containerPath": "/var/opt/gitlab/gitlab-rails/shared",
          "hostPath": "/mnt/srv/gitlab-data/shared/",
          "mode": "RW"
        },
        {
          "persistent": {
            "type": "root",
            "size": 1000,
            "constraints": []
          },
          "mode": "RW",
          "containerPath": "builds"
        },
        {
          "containerPath": "/var/opt/gitlab/gitlab-ci/builds",
          "hostPath": "/mnt/srv/gitlab-data/builds/",
          "mode": "RW"
        }
      ],
      "docker": {
        "image": "bootstrap1.live.fiinlab.local:8083/jaakit_gitlab_pro:0.0.2",
        "privileged": true,
        "parameters": [
          {
            "key": "log-driver",
            "value": "none"
          }
        ]
      },
      "portMappings": [
        {
            "hostPort": 10112,
            "containerPort": 80,
            "protocol": "tcp"
        }
    ]
    },
    "networks": [
      {
        "mode": "container/bridge"
      }
    ],
    "instances": 1,
    "cpus": 2,
    "mem": 4098,
    "labels":{
      "HAPROXY_DEPLOYMENT_GROUP":"gitlab",
      "HAPROXY_DEPLOYMENT_ALT_PORT":"10006",
      "HAPROXY_GROUP":"external",
      "HAPROXY_0_REDIRECT_TO_HTTPS":"true",
      "HAPROXY_0_STICKY":"true",
      "HAPROXY_0_VHOST": "fiinlab.jaak-it.com"
    },
    "fetch":[
      { "uri": "file:///etc/docker.tar.gz" }
    ]
  }